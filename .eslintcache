ableContainer}>\r\n      <Table className={classes.table} aria-label=\"simple table\">\r\n        <TableBody>\r\n          <TableRow>\r\n            <TableCell>Activity</TableCell>\r\n            <TableCell align=\"center\">{activityText}</TableCell>\r\n          </TableRow>\r\n          <TableRow>\r\n            <TableCell>Goal</TableCell>\r\n            <TableCell align=\"center\">{goalText}</TableCell>\r\n          </TableRow>\r\n          <TableRow>\r\n            <TableCell>Total calories</TableCell>\r\n            <TableCell align=\"center\">{totalCalories} kcal</TableCell>\r\n          </TableRow>\r\n        </TableBody>\r\n      </Table>\r\n    </TableContainer>\r\n  );\r\n}\r\n","C:\\Users\\Wiktoria\\Desktop\\PORTFOLIO\\diet-app\\src\\components\\MealsCheckbox\\MealsCheckbox.tsx",["175","176"],"import React, { useState, FunctionComponent, ChangeEvent } from \"react\";\r\nimport { Checkbox } from \"primereact/checkbox\";\r\nimport { useSelector } from \"react-redux\";\r\n\r\nimport  \"./MealsCheckbox.css\";\r\nimport { RootState } from \"../../index\";\r\n\r\ninterface MealsCheckboxProps {\r\n  onMealChange: (e: PrimeCheckboxProps) => void;\r\n}\r\n\r\nexport interface PrimeCheckboxProps {\r\n  originalEvent: Event;\r\n  value: any;\r\n  checked: boolean;\r\n  target: {\r\n    type: string;\r\n    name: string;\r\n    id: string;\r\n    value: any;\r\n    checked: boolean;\r\n  };\r\n}\r\n\r\nconst MealsCheckbox: FunctionComponent<MealsCheckboxProps> = ({\r\n  onMealChange,\r\n}) => {\r\n  const { meals } = useSelector((state: RootState) => state.userDataReducer);\r\n\r\n  return (\r\n    <div>\r\n      <div className=\"card\">\r\n        <h4>Choose your meals:</h4>\r\n        <div className='selectMealsField'>\r\n          <div className=\"p-field-checkbox\">\r\n            <Checkbox\r\n              inputId=\"meal1\"\r\n              name=\"meal\"\r\n              value=\"Breakfast\"\r\n              onChange={onMealChange}\r\n              checked={meals.